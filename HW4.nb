      SuperscriptBox["1.61", "1.11"], 
      RowBox[{"\[CapitalGamma]", "(", "1.11", ")"}]], "]"}], 
    SuperscriptBox["\[Theta]", 
     RowBox[{"1.11", "-", "1"}]], 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "1.61"}], "\[Theta]"}]], 
